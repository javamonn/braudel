{
  "name": "Braudel",
  "description": "Full-text search your browsing history.",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "ServiceWorker.js"
  },
  "permissions": ["storage", "unlimitedStorage", "webNavigation", "scripting"],
  "host_permissions": ["<all_urls>"]
}
